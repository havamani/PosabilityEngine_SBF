<build>
<plugins>
<plugin>
<groupId>org.springframework.boot</groupId>
<artifactId>spring-boot-maven-plugin</artifactId>
<configuration>
			        <source>1.8</source>
			        <target>1.8</target>
			        <encoding>ISO-8859-1</encoding>
			    </configuration>
</plugin>
 </plugins>
</build>
<profiles>
        <profile>
            <id>release</id>
            <build>
                <plugins>
                    <plugin>
                       <artifactId>maven-war-plugin</artifactId>
  <version>3.0.0</version>
                        <configuration>
                        <failOnMissingWebXml>false</failOnMissingWebXml>
                            <finalName>PosabilityEngine_SBF-${project.version}</finalName>
                        </configuration>
                  </plugin>
                 
                </plugins>
            </build>
        </profile>
    </profiles>
